#include <iostream>
#include<stack>
#include<math.h>
using namespace std;

int cal(int v1, int v2, char op){
    if(op == '^') return pow(v1, v2);
    if(op =='*') return v1*v2;
    if(op == '/') return v1/v2;
    if(op == '+') return v1+v2;
    else return v1-v2;
}

int eval(string &str){
    stack <int>st;
    for(int i=str.size()-1;i>=0;i--){
        char ch = str[i];
        if(isdigit(ch)) {
            st.push(ch - '0');
        }
        else{
            int v1 = st.top();    //correctly debug to check why we have taken v1 as first popped out element.
            st.pop();
            int v2 = st.top();
            st.pop();
            st.push(cal(v1, v2, ch));
        }
    }
    return st.top();
}

int main() {
    string s = "++-1234";
    cout<<eval(s)<<endl;
    return 0;
}
