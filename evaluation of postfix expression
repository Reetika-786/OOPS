#include <iostream>
#include<stack>
#include<math.h>
using namespace std;

int cal(int v1, int v2, char op){
    if(op == '^') return (int) pow(v1, v2);
    if(op =='*') return v1*v2;
    if(op == '/') return v1/v2;
    if(op == '+') return v1+v2;
    else return v1-v2;
}

int eval(string &str){
    stack <int>st;
    for(int i=0;i<str.size();i++){
        char ch = str[i];
        if(isdigit(ch)) {
            st.push(ch - '0');
        }
        else{
            int v2 = st.top();    //pop v2 first and then v1, debug to check for the reason
            st.pop();
            int v1 = st.top();
            st.pop();
            st.push(cal(v1, v2, ch));
        }
    }
    return st.top();
}

int main() {
    string s = "12-34+*";
    cout<<eval(s)<<endl;
    return 0;
}
