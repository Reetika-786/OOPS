void insertAtBottom(stack <int> &st, int x){
    stack <int> temp;
    while(not st.empty()){
        int curr = st.top();
        st.pop(); 
        temp.push(curr);
    }
    st.push(x);
    while(not temp.empty()){
        int curr = temp.top();
        temp.pop();
        st.push(curr);
    }
}

void reverse(stack <int> &st){
    if(st.empty()) return;
    int curr = st.top();
    st.pop();
    reverse(st);
    insertAtBottom(st, curr);
}
