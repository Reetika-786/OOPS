void insertAtIndex(stack<int> st, int x, int idx){
    stack<int> temp;
    int n = st.size();
    int count =0;
    while(count<n-idx){
        count++;
        int curr = st.top();
        st.pop();
        temp.push(curr);
    }
    st.push(x);
    while(not temp.empty()){
        int curr = temp.top();
        temp.pop();
        st.push(curr);
    }
}
